<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet  author="liquibase-1"  id="avinash" runOnChange="true">
        <tagDatabase  tag="${REQUEST_NUMBER}-${BUILD_NUMBER}"/>
    </changeSet>
<changeSet author="avinash"  id="executeCommand-exp" runOnChange="true">  
    <executeCommand  executable="dbscripts/exp.sh"  
            timeout="300000">  
        <arg value="LIQUIBASE_DEV01"/>
        <arg value="oracle-db:1521/XEPDB1"/> 
        <arg value="LIQUIBASE_DEV01"/> 
        <arg value="DUMPFILE.dmp"/> 
        <arg value="LOGFILE.log"/> 
        <arg value="admin123"/>
    </executeCommand>  
</changeSet>

</databaseChangeLog>
